pipeline {
    agent any

    stages {
        stage ("Git Checkout") {
            steps {
                git branch: "main", url: "https://github.com/abcdsantu/01_JTerraform.git"
            }
        }

        stage ("Initialise the Terraform and apply on workspace - Dev") {
            steps{
                sh 'terraform init'
                
            }
        } 
    }
}


